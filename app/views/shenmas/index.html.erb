<h1>Listing shenmas</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shenmas.each do |shenma| %>
      <tr>
        <td><%= link_to 'Show', shenma %></td>
        <td><%= link_to 'Edit', edit_shenma_path(shenma) %></td>
        <td><%= link_to 'Destroy', shenma, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shenma', new_shenma_path %>
